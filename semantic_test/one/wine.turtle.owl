:Wine rdf:type owl:Class ;
      
      rdfs:label "wine"@en ,
                 "vin"@fr ;
      
      rdfs:subClassOf food:PotableLiquid ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :hasMaker ;
                        owl:cardinality "1"^^xsd:nonNegativeInteger
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :hasFlavor ;
                        owl:cardinality "1"^^xsd:nonNegativeInteger
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :hasBody ;
                        owl:cardinality "1"^^xsd:nonNegativeInteger
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :hasSugar ;
                        owl:cardinality "1"^^xsd:nonNegativeInteger
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :locatedIn ;
                        owl:someValuesFrom :Region
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :madeFromGrape ;
                        owl:minCardinality "1"^^xsd:nonNegativeInteger
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :hasMaker ;
                        owl:allValuesFrom :Winery
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :hasColor ;
                        owl:cardinality "1"^^xsd:nonNegativeInteger
                      ] .



###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#WineBody

:WineBody rdf:type owl:Class ;
          
          owl:equivalentClass [ rdf:type owl:Class ;
                                owl:oneOf ( :Full
                                            :Medium
                                            :Light
                                          )
                              ] ;
          
          rdfs:subClassOf :WineTaste .



###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#WineColor

:WineColor rdf:type owl:Class ;
           
           owl:equivalentClass [ rdf:type owl:Class ;
                                 owl:oneOf ( :Red
                                             :Rose
                                             :White
                                           )
                               ] ;
           
           rdfs:subClassOf :WineDescriptor .



###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#WineDescriptor

:WineDescriptor rdf:type owl:Class ;
                
                owl:equivalentClass [ rdf:type owl:Class ;
                                      owl:unionOf ( :WineColor
                                                    :WineTaste
                                                  )
                                    ] ;
                
                rdfs:comment "Made WineDescriptor unionType of tastes and color" .



###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#WineFlavor

:WineFlavor rdf:type owl:Class ;
            
            owl:equivalentClass [ rdf:type owl:Class ;
                                  owl:oneOf ( :Strong
                                              :Delicate
                                              :Moderate
                                            )
                                ] ;
            
            rdfs:subClassOf :WineTaste .



###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#WineGrape

:WineGrape rdf:type owl:Class ;
           
           rdfs:subClassOf food:Grape .



###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#WineSugar

:WineSugar rdf:type owl:Class ;
           
           owl:equivalentClass [ rdf:type owl:Class ;
                                 owl:oneOf ( :OffDry
                                             :Dry
                                             :Sweet
                                           )
                               ] ;
           
           rdfs:subClassOf :WineTaste .



###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#WineTaste

:WineTaste rdf:type owl:Class ;
           
           rdfs:subClassOf :WineDescriptor .



###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#Winery

:Winery rdf:type owl:Class .



###  http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#Zinfandel

:Zinfandel rdf:type owl:Class ;
           
           owl:equivalentClass [ rdf:type owl:Class ;
                                 owl:intersectionOf ( :Wine
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty :madeFromGrape ;
                                                        owl:hasValue :ZinfandelGrape
                                                      ]
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty :madeFromGrape ;
                                                        owl:maxCardinality "1"^^xsd:nonNegativeInteger
                                                      ]
                                                    )
                               ] ;
           
           rdfs:subClassOf [ rdf:type owl:Restriction ;
                             owl:onProperty :hasFlavor ;
                             owl:allValuesFrom [ rdf:type owl:Class ;
                                                 owl:oneOf ( :Strong
                                                             :Moderate
                                                           )
                                               ]
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :hasBody ;
                             owl:allValuesFrom [ rdf:type owl:Class ;
                                                 owl:oneOf ( :Full
                                                             :Medium
                                                           )
                                               ]
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :hasColor ;
                             owl:hasValue :Red
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :hasSugar ;
                             owl:hasValue :Dry
                           ] .





