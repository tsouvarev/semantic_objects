                                ] ;
            
            rdfs:subClassOf [ rdf:type owl:Restriction ;
                              owl:onProperty :madeFromGrape ;
                              owl:hasValue :GamayGrape
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :hasSugar ;
                              owl:hasValue :Dry
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :hasBody ;
                              owl:hasValue :Light
